{
  "name": "cozy-pass",
  "version": "1.0.0",
  "engines": [
    "node >= 0.10.0"
  ],
  "description": "A template project for Cozy applications written in JavaScript",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/cozy/cozy-template.git"
  },
  "main": "server.js",
  "dependencies": {
    "americano": "0.4.2",
    "cozydb": "0.1.3",
    "lesspass": "1.1.1"
  },
  "devDependencies": {
    "cozy-fixtures": "1.0.1",
    "jslint": "0.9.3",
    "mocha": "2.3.3",
    "request-json": "0.5.3",
    "should": "7.1.1"
  },
  "scripts": {
    "test": "mocha tests/*.js",
    "start": "node server.js"
  },
  "cozy-permissions": {
    "User": {
      "description": "Retrieve the Cozy's email address for password generation"
    },
    "Lesspass": {
      "description": "Password storage"
    }
  },
  "cozy-displayName": "Password Management"
}
